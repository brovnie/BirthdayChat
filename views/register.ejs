
<div>
    <h2>Sign up</h2>

     <div>
            <label for="firstname">First Name</label>
            <input type="text" name="firstname" id="" placeholder="Firstname">
        </div>
        <div>
            <label for="lastname">Last Name</label>
            <input type="text" name="lastname" id="" placeholder="lastname">
        </div>
        <div>
            <label for="dateofbirth">Date of birth</label>
            <input type="date" name="dateofbirth" id="" data-date-format="DD MMMM YYYY">
        </div>
        <div>
            <label for="username">Email</label>
            <input type="text" name="username" id="" placeholder="Enter email or username">
        </div>
        <div>
            <label for="password">Password</label>
            <input type="password" name="password" id="" placeholder="Password">
        </div>
        <div>
            <label for="password2">Repeat Password</label>
            <input type="password" name="password2" id="" placeholder="Repeat Password">
        </div>
        <input type="submit" value="Submit"> 

        <form action="/register" method="POST">
            <input type="text" name="username" placeholder="username">
            <input type="password" name="password" placeholder="password">
            <button>Sign up</button>
        </form>

   
</div>